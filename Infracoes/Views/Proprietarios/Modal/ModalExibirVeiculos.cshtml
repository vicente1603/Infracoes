<div class="modal fade" data-app-modal="proprietarios.modal.exibirVeiculos" data-backdrop="static">
    <div class="modal-dialog">
        <div class="modal-content">
            <form name="proprietarios.form.dados">
                <div class="modal-header">
                    <button type="button" class="close" data-app-modal-dismiss="proprietarios.modal.exibirVeiculos"><span>&times;</span></button>
                    <h4 class="modal-title">Lista de Veículos</h4>
                </div>
                <table class="table table-striped">
                    <thead>
                        <tr>
                            <th>Veículo</th>
                            <th>Placa</th>
                            <th>Estado</th>

                        </tr>
                    </thead>
                    <tbody>
                        <tr data-ng-repeat="veiculo in proprietarios.proprietario.veiculos">
                            <td data-ng-bind="veiculo.idVeiculo"></td>
                            <td data-ng-bind="veiculo.placa"></td>
                            <td data-ng-bind="veiculo.uf"></td>
                            <td data-ng-bind="veiculo.modelo"></td>
                            <td data-ng-bind="veiculo.modelo"></td>
                        </tr>
                    </tbody>
                </table>
                <div class="modal-footer">
                    <span>&nbsp;</span>
                    <button type="button" class="btn btn-danger" data-app-modal-dismiss="proprietarios.modal.exibirVeiculos">
                        Fechar
                    </button>
                </div>
            </form>
        </div>
    </div>
</div>

